{
  "name": "icx",
  "module": "./src/index.ts",
  "type": "module",
  "scripts": {
    "prepack": "bun test && bun run build",
    "test": "bun test $ bun test --reporter=junit --reporter-outfile=./tests/reports/report.xml",
    "watch": "bun --watch test --bail",
    "lint": "bunx --bun biome lint",
    "fix": "bunx --bun biome check . --write --unsafe",
    "format": "bunx --bun biome format --write",
    "format:check": "bunx --bun biome format"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "husky": "^9.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vite": "^7.1.12",
    "tsc-alias": "^1.8.16"
  },
  "dependencies": {
    "@stationeers-ic/exact-ic10-math": "^1.1.1",
    "eventemitter3": "^5.0.1",
    "i18next": "^25.6.0",
    "valibot": "^1.1.0"
  }
}