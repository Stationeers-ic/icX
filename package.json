{
	"name": "icx-compiler",
	"version": "2.1.6",
	"description": "Compiler icX to ic10 ",
	"main": "src/index.js",
	"scripts": {
		"build": "babel src -d src",
		"whatch": "tsc -w",
		"test": "ts-node ./tests/test.ts",
		"jest": "jest tests/jest.js",
		"debug": "nodemon -w ./ -e js,icX jest"
	},
	"transform": {
		"\\.js$": "<rootDir>/node_modules/babel-jest"
	},
	"homepage": "https://github.com/Stationeers-ic/icX",
	"keywords": [
		"ic10",
		"icX",
		"compiller"
	],
	"files": [
		"src/*",
		"src/index.ts",
		"index.d.ts",
		"index.js",
		"index.js.map",
		"test.ts"
	],
	"exclude": [
		".idea",
		".vscode",
		".ic10",
		".icX",
		"tests"
	],
	"module": "commonjs",
	"author": "Traineratwot",
	"license": "MIT",
	"devDependencies": {
		"@jest/globals": "^29.3.1",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.15.11",
		"chalk": "4.1.2",
		"ic10": "^2.1.3",
		"jest": "^29.3.1",
		"ts-jest": "^29.0.3",
		"typescript": "^4.9.3"
	},
	"dependencies": {
		"@types/luaparse": "^0.2.9",
		"luaparse": "^0.3.1"
	}
}
